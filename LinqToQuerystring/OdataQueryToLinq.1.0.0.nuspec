	<files>
		<file src="bin\Release\LinqToQuerystring.dll" target="lib\net35\LinqToQuerystring.dll" />
	</files>
    <dependencies>
        <dependency id="Antlr" version="3.5.0.2" />
    </dependencies>
</package>